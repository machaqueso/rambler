@model Rambler.Models.Twitch.TwitchUserData

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Twitch User</h2>

<div class="row">
    <label>Id</label>
    <div>@Model.id</div>
</div>
<div class="row">
    <label>Name</label>
    <div>@Model.login</div>
</div>
<div class="row">
    <label>Display Name</label>
    <div>@Model.display_name</div>
</div>
<div class="row">
    <label>Type</label>
    <div>@Model.type</div>
</div>
<div class="row">
    <label>Broadcaster type</label>
    <div>@Model.broadcaster_type</div>
</div>
<div class="row">
    <label>Description</label>
    <div>@Model.description</div>
</div>
<div class="row">
    <label>Creation date</label>
    <div>@Model.created_at</div>
</div>
